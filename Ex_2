public class fibo_recursive {
	
	 public static int fibonacci(int n) {
		    if (n == 1 || n == 2) {
		      return 1;
		    }
		 
		    // fibonacci(n) = fibonacci(n-2) + fibonacci(n-1)
		    return fibonacci(n-2) + fibonacci(n-1);
		  }
	 
	public static void main(String[] args) {
		for (int i = 1; i <= 20; i++) {
		      System.out.print(fibonacci(i) + " ");
		    }
		    System.out.println();

	}

}
