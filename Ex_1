import java.util.Scanner;

public class Ex_1 {

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        System.out.println("Introdu un numar: ");
        int n = input.nextInt();
        
        int sum = 0;
        for(int i=2;i<n;i++)  {
            if((i % 3==0 || i % 5 ==0)){
                sum += i;
            }
        }
        System.out.println(sum);
    }
}
